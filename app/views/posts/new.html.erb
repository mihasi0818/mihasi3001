          <style>
            body {
              background-image: url(/images/grond/YuZhongSkins.webp);
              background-size: cover;
            }

            .content_post {
              display: flex;
              justify-content: center;
              height: 100px;
            }

            .content {

              width: 65%;
            }

            .actions {
              display: flex;
              justify-content: center;
              padding: 20px;
            }

            .scary-button {
              background-color: rgb(128, 0, 255);
              color: white;
              padding: 10px 20px;
              font-size: 16px;
              border-radius: 5px;
              border: none;
              transition: all 0.3s ease-in-out;
              position: relative;
            }

            .scary-button:hover {
              transform: scale(1.1);
            }

            .scary-button:active {
              transform: scale(0.9);
            }

            .scary-button:focus {
              outline: none;
            }

            .scary-button:active:after {
              content: "";
              background-image: url('https://i.imgur.com/RL7VU1M.png');
              background-size: cover;
              width: 200px;
              height: 200px;
              position: absolute;
              left: -100px;
              top: -30px;
              animation: scary-emoji 1s linear 1;
            }

            @keyframes scary-emoji {
              0% {
                transform: rotate(0deg) scale(1);
              }

              25% {
                transform: rotate(-45deg) scale(1.2);
              }

              50% {
                transform: rotate(0deg) scale(1);
              }

              75% {
                transform: rotate(45deg) scale(1.2);
              }

              100% {
                transform: rotate(0deg) scale(1);
              }
            }

            p {
              animation-name: p;
              animation-duration: 4s;
              animation-iteration-count: infinite;
            }

            @keyframes p {
              from {
                background-color: rgb(90, 0, 147);
              }

              to {
                background-color: rgb(62, 0, 80);
              }
            }

            @media (max-width: 576px) {
              input[type=radio]+label:before {
                width: 35px;
                height: 45px;
              }
            }
           
          </style>

